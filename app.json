{
  "expo": {
    "name": "CryptoTrackerLite",
    "slug": "CryptoTrackerLite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/2.png",
    "scheme": "cryptotrackerlite",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eunock.cryptotrackerlite"
    },
    "android": {
      "package": "com.eunock.cryptotrackerlite",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/2.png",
        "backgroundColor": "#0D0F14"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D0F14"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0225af35-479e-4c7a-8ad4-32e8ddd1fd62"
      }
    }
  }
}
